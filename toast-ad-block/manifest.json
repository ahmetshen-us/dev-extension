{
  "manifest_version": 2,
  "name": "Lynda Toast Ad Block",
  "version": "0.0.1",
  "description": "Disable toat ads on Lynda",
  "content_scripts": [
    {
        "matches": [
        "http://*.lynda.com/*","https://*.lynda.com/*","http://*.amazon.com/*","https://*.amazon.com/*"],
        "js": ["main.js"],
        "run_at": "document_end",
        "all_frames": true
    }
  ]
}